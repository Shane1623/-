class Personel { 
private String name; private int bornin; 
public Personel(String name, int bornin) { 
} 
this.name = name; 
this.bornin = bornin; 
// Getter and Setter methods for name 
and bornin 
public void displayInfo() { 
System.out.println("Name: " + name); System.out.println("Born In: " + bornin); 
} 
} 
class Student extends Personel { 
private int id; 
private int grade; 
private String c1; private String c2; 
public Student(String name, int bornin, int id, int grade, String c1, String c2) { 
} 
super(name, bornin); 
this.id = id; 
this.grade = grade; 
this.c1 = c1; 
this.c2 = c2; 
// Getter and Setter methods for id, grade, c1, and c2 
public void displayStudentInfo() { 
super.displayInfo(); 
System.out.println("ID: " + id); 
System.out.println("Grade: " + grade); 
System.out.println("Course 1: " + c1); 
System.out.println("Course 2: " + c2); 
} 
} 
class Teacher extends Personel { 
private String course; 
public Teacher(String name, int bornin, String course) { 
} 
super(name, bornin); 
this.course = course; 
// Getter and Setter methods for course 
public void displayTeacherInfo() { 
super.displayInfo(); 
System.out.println("Course Taught: " + 
course); 
} 
} 
public class Main { 
public static void main(String[] args) { 
Student student = new Student("Alice", 2000, 12345, 10, "Math", "Science"); 
Teacher teacher = new Teacher("Mr. 
Smith", 1980, "History"); 
student.displayStudentInfo(); System.out.println(); 
teacher.displayTeacherInfo(); 
} 
} 

